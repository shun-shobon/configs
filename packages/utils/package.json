{
  "name": "@shun-shobon/eslint-config-utils",
  "version": "1.0.1",
  "description": "My shareable ESLint config (utilities)",
  "author": "Shuntaro Nishizawa <me@s2n.tech>",
  "repository": {
    "type": "git",
    "url": "https://github.com/shun-shobon/eslint-config.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./globs": {
      "import": "./dist/globs.js",
      "require": "./dist/globs.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/globs.ts"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@typescript-eslint/utils": "^6.13.2"
  }
}
