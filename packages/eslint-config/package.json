{
	"name": "@shun-shobon/eslint-config",
	"version": "0.13.1",
	"type": "module",
	"description": "shun-shobon's ESLint config",
	"repository": {
		"directory": "packages/eslint-config",
		"type": "git",
		"url": "https://github.com/shun-shobon/eslint-config.git"
	},
	"homepage": "https://github.com/shun-shobon/eslint-config/packages/eslint-config#readme",
	"author": "NISHIZAWA Shuntaro <me@s2n.tech> (https://s2n.tech)",
	"license": "MIT",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup-node src/index.ts",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"inspect": "pnpm run build && eslint-config-inspector",
		"lint": "eslint .",
		"lint:ci": "eslint --max-warnings 0 .",
		"lint:fix": "eslint --fix ."
	},
	"peerDependencies": {
		"eslint": "^8.21.0 || ^9.0.0"
	},
	"dependencies": {
		"@eslint/js": "9.4.0",
		"@next/eslint-plugin-next": "14.2.3",
		"astro-eslint-parser": "1.0.2",
		"eslint-config-flat-gitignore": "0.1.5",
		"eslint-define-config": "2.1.0",
		"eslint-import-resolver-typescript": "3.6.1",
		"eslint-plugin-astro": "1.2.2",
		"eslint-plugin-import-x": "0.5.1",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-n": "17.8.1",
		"eslint-plugin-qwik": "1.5.6",
		"eslint-plugin-react": "7.34.2",
		"eslint-plugin-react-hooks": "4.6.2",
		"eslint-plugin-regexp": "2.6.0",
		"eslint-plugin-simple-import-sort": "12.1.0",
		"eslint-plugin-storybook": "0.8.0",
		"eslint-plugin-unicorn": "53.0.0",
		"globals": "15.4.0",
		"local-pkg": "0.5.0",
		"typescript-eslint": "7.13.0"
	},
	"devDependencies": {
		"@eslint/config-inspector": "0.4.11"
	},
	"sideEffects": false
}
