{
	"name": "@shun-shobon/eslint-config",
	"version": "0.8.0",
	"type": "module",
	"description": "shun-shobon's ESLint config",
	"repository": {
		"type": "git",
		"url": "https://github.com/shun-shobon/eslint-config.git",
		"directory": "packages/eslint-config"
	},
	"homepage": "https://github.com/shun-shobon/eslint-config/packages/eslint-config#readme",
	"author": "NISHIZAWA Shuntaro <me@s2n.tech> (https://s2n.tech)",
	"license": "MIT",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsup-node src/index.ts",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"lint:fix": "eslint --fix ."
	},
	"peerDependencies": {
		"eslint": "^8.21.0"
	},
	"dependencies": {
		"@eslint/js": "^8.57.0",
		"@next/eslint-plugin-next": "^14.2.2",
		"astro-eslint-parser": "^0.17.0",
		"eslint-config-flat-gitignore": "^0.1.5",
		"eslint-define-config": "^2.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-astro": "^0.34.0",
		"eslint-plugin-import": "npm:eslint-plugin-i@^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-unicorn": "^51.0.1",
		"globals": "^15.0.0",
		"local-pkg": "^0.5.0",
		"typescript-eslint": "^7.7.0"
	},
	"devDependencies": {
		"@types/eslint__js": "^8.42.3"
	},
	"sideEffects": false
}
